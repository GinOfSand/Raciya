<Window x:Class="Raciya.ConnectSetting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Raciya"
        mc:Ignorable="d"
        Title="ConnectSetting" Height="200" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox x:Name="IP_TB" Text="IP:" Grid.Row="0"></TextBox>
        <TextBox x:Name="Port_TB" Text="Port:" Grid.Row="1"></TextBox>
        <Button Content="Ok" x:Name="OKBTN" Grid.Row="2" Click="OKBTN_Click"></Button>
        <Button Content="Cancel" x:Name="CancelBTN" Grid.Row="3" Click="CancelBTN_Click"></Button>
    </Grid>
</Window>
